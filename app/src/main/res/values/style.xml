<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.MAD3125_TeamSundry_FinalProject.RemoveSplashScreenTheme" parent="@style/Theme.MAD3125_TeamSundry_FinalProject">
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="SearchViewStyle" parent="Widget.AppCompat.SearchView">
        <item name="searchIcon">@drawable/ic_search</item>
        <item name="queryBackground">@color/textColorPrimary</item>
        <item name="searchHintIcon">@null</item>
        <item name="queryHint">@string/search</item>
        <item name="android:textColorPrimary">@color/textColorPrimary</item>
        <item name="android:textColorHint">@color/textColorDisabled</item>
        <item name="android:tint">@color/textColorPrimary</item>
    </style>
    <style name="MyLabel">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>
</resources>